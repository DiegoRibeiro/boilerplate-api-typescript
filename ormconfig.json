{
   "type": "mysql",
   "host": "<host>",
   "port": 3306,
   "username": "<user>",
   "password": "<pass>",
   "database": "<db>",
   "synchronize": true,
   "logging": false,
   "entities": [
      "app/entity/**/*.js"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}